    <label asp-for="UserName" class="form-label">User</label>
    <div class="form-control">@Model.UserName</div>
</div>

<div class="mb-3">
    <label asp-for="Type" class="form-label"></label>
    <div class="form-control">@Model.Type</div>
</div>

<div class="mb-3">
    <label asp-for="Time" class="form-label"></label>
    <div class="form-control">@Model.Time</div>
</div>

<div class="mb-3">
    <label asp-for="Description" class="form-label"></label>
    <div class="form-control">@Model.Description</div>
</div>
